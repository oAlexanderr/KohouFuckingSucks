-- Scripts:

local function JMKXGI_fake_script() -- Era.Script 
	local script = Instance.new('Script', Era)

	local Era = script.Parent
	local Alex = script.Parent.Parent.Alex
	
	local Intro = script.Parent.Parent.Parent.MS.Intro.Era
	local Floor = script.Parent.Parent.Parent.MS.Floor.Era
	local TableType = script.Parent.Parent.Parent.MS.TableType.Era
	local Outro = script.Parent.Parent.Parent.MS.Outro.Era
	
	
	function lclick()
		Alex.Script.Disabled = true
	
		Intro.Disabled = false 
		Floor.Disabled = false
		TableType.Disabled = false
		Outro.Disabled = false
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(JMKXGI_fake_script)()
local function DKCRN_fake_script() -- Alex.Script 
	local script = Instance.new('Script', Alex)

	local Alex = script.Parent
	local Era = script.Parent.Parent.Era
	
	local Intro = script.Parent.Parent.Parent.MS.Intro.Alex
	local Floor = script.Parent.Parent.Parent.MS.Floor.Alex
	local TableType = script.Parent.Parent.Parent.MS.TableType.Alex
	local Outro = script.Parent.Parent.Parent.MS.Outro.Alex
	
	
	function lclick()
		Era.Script.Disabled = true
		
		Intro.Disabled = false 
		Floor.Disabled = false
		TableType.Disabled = false
		Outro.Disabled = false
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(DKCRN_fake_script)()
local function KSSWFG_fake_script() -- Intro.Alex 
	local script = Instance.new('Script', Intro)

	function lclick()
		local args = {
			[1] = "Hello, and welcome to Kohaú! My name is Lex, and I will be serving you today. How many people will be accommodating you today?",
			[2] = "All"
		}
	
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	end
	
	script.Parent.MouseButton1Click:Connect(lclick)
end
coroutine.wrap(KSSWFG_fake_script)()
