local function BXTM_fake_script() -- Left.LeftButton 
	local script = Instance.new('Script', Left)

	function onclick()
		print("fuckleft")
		script.Parent.Parent.Parent.Backing.Pages.MS.Visible = false
		script.Parent.Parent.Parent.Backing.Pages.Settings.Visible = true
	end
	
	script.parent.MouseButton1Click:connect(onclick)
end
coroutine.wrap(BXTM_fake_script)()
local function RJONT_fake_script() -- Right.RightButton 
	local script = Instance.new('Script', Right)

	function onclick()
		print("fuckright")
		script.Parent.Parent.Parent.Backing.Pages.MS.Visible = true
		script.Parent.Parent.Parent.Backing.Pages.Settings.Visible = false
	end
	
	script.parent.MouseButton1Click:connect(onclick)
end
coroutine.wrap(RJONT_fake_script)()
